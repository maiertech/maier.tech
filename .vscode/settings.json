{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.defaultScriptLanguage": "ts",
  "thunder-client.saveToWorkspace": true,
  "grammarly.files.include": ["**/README.md", "**/*.md"],
  "grammarly.config.documentDialect": "auto-text",
  "grammarly.config.suggestions.ConjunctionAtStartOfSentence": true,
  "grammarly.config.suggestions.PassiveVoice": true,
  "grammarly.config.suggestions.UnnecessaryEllipses": true,
  "grammarly.config.suggestions.StylisticFragments": true
}
